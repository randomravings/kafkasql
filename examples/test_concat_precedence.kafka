create context test;

use context test;

create type Test as struct (
    a STRING,
    b STRING,
    c INT32
)
constraint test1 (check (a || b = 'ab'))
constraint test2 (check (a || (c + 1) <> 'test'))
constraint test3 (check (a || b || 'suffix' <> ''))
constraint test4 (check ((a || b = 'x') or (a || b = 'y')))
